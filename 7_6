#include<iostream>
using namespace std;
void insertstr(char *a,char *b,int n)
{
	int i;
	for (i = 0; i<n-1 ; i++)
	{
		cout << a[i];
	}
	cout << b;
	for (i = n-1; a[i]!='\0'; i++)
	{
		cout << a[i];
	}

}
int main()
{
	int n;
	char str1[50];
	char str2[50];
	cout << "输入两串字符 \n";
		cin >> str1;
		cin >> str2;
	cout << "输入插入位置 \n";
		cin >> n;
		insertstr(str1, str2, n);
	cout << "\n";
}
